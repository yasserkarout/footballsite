<%= simple_form_for(@match) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :sport %>
    <%= f.input :description %>
    <%= f.input :numOfPlayers ,label: "Number of Players" %>
    <%= f.input :address%>
    <%= f.input :date %>
  </div>

  <div class="form-actions">
    <%= f.button :submit ,class: "btn btn-primary" %>
  </div>
<% end %>
